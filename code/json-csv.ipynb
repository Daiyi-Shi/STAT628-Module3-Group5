    "import json\n",
    "\n",
    "for data in ['business_city', 'review_city', 'tip_city', 'user_city']:\n",
    "    with open(f'{data}.json') as f:\n",
    "        data_df = pd.DataFrame([json.loads(line) for line in f.readlines()])\n",
    "        data_df.to_csv(f'{data}.csv')"
